{
    "name": "l4logger/laravel",
    "description": "Official L4Logger Log monitoring & exception reporter for Laravel",
    "homepage": "https://github.com/l4logger/laravel",
    "license": "MIT",
    "keywords": [
        "l4logger",
        "laravel",
        "logging",
        "log",
        "exceptions",
        "errors",
        "logging",
        "tracking"
    ],
    "authors": [
        {
            "name": "Abdul Qadir",
            "email": "abdulqadirshaikh101@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5"
    },
    "autoload": {
        "psr-4": {
            "L4Logger\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "L4Logger\\Laravel\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
